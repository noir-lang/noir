{
  "name": "docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "preprocess": "yarn workspace @noir-lang/acvm_js build && ./scripts/codegen_nargo_reference.sh && yarn node ./scripts/preprocess/index.js",
    "dev": "yarn preprocess && ENV=dev docusaurus start",
    "build": "yarn preprocess && docusaurus build",
    "clean": "rm -rf ./processed-docs ./processed-docs-cache ./build",
    "version::stables": "ts-node ./scripts/setStable.ts",
    "serve": "serve build",
    "production:serve": "mkdir -p temp-serve/docs && cp -r build/* temp-serve/docs/ && serve temp-serve -l 3000 && rm -rf temp-serve",
    "swizzle": "docusaurus swizzle",
    "cut_version": "yarn version::stables && ./scripts/cut_version.sh"
  },
  "dependencies": {
    "@cookbookdev/docsbot": "^4.24.9",
    "@docusaurus/core": "^3.9.2",
    "@docusaurus/preset-classic": "^3.9.2",
    "@easyops-cn/docusaurus-search-local": "^0.52.2",
    "@mdx-js/react": "^3.1.1",
    "axios": "^1.13.2",
    "clsx": "^1.2.1",
    "hast-util-is-element": "^3.0.0",
    "prism-react-renderer": "^2.1.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-spinners": "^0.17.0",
    "rehype-katex": "^7.0.1",
    "remark-math": "^6.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.9.2",
    "@docusaurus/tsconfig": "^3.9.2",
    "@docusaurus/types": "^3.9.2",
    "@types/prettier": "^3.0.0",
    "docusaurus-plugin-llms": "^0.2.2",
    "docusaurus-plugin-typedoc": "1.4.2",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "3.7.4",
    "serve": "^14.2.1",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.15",
    "typedoc-plugin-frontmatter": "^1.3.1",
    "typedoc-plugin-markdown": "4.9.0",
    "typedoc-plugin-merge-modules": "^7.0.0",
    "typescript": "^5.8.3"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=22.21.1"
  }
}
