type Double<let N: u32> = N * 2;

pub struct Foo<T, let N: u32> {
    a: T,
    b: [Field; N],
}

fn main(x: Field) {
    let a: Foo<Field, 4 * 2> = foo::<4>(x);
    assert(a.a == x);
}

fn foo<let N: u32>(x: Field) -> Foo<Field, Double<N>> {
    Foo::<Field, N * 2> { a: x, b: [1_Field; N * 2] }
}
