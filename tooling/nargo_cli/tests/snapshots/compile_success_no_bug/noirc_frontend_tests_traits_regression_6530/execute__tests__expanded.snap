pub trait From2<T> {
    fn from2(input: T) -> Self;
}

pub trait Into2<T> {
    fn into2(self) -> T;
}

impl<T, U> Into2<T> for U
where
    T: From2<U>,
{
    fn into2(self) -> T
    where
        T: From2<Self>,
    {
        T::from2(self)
    }
}

