struct Array<T, let N: u32> {
    inner: [T; N],
}

impl<let N: u32, T> Array<T, N> {
    pub fn len(self) -> u32 {
        N as u32
    }
}

fn main(xs: [Field; 2]) {
    let ys: Array<Field, 2> = Array::<Field, 2> { inner: xs };
    assert(ys.len() == 2);
}
