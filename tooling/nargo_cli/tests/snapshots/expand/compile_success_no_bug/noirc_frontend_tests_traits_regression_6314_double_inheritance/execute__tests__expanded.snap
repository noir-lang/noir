trait Foo {
    fn foo(self) -> Self;
}

impl Foo for Field {
    fn foo(self) -> Self {
        self + 1
    }
}

trait Bar {
    fn bar(self) -> Self;
}

impl Bar for Field {
    fn bar(self) -> Self {
        self + 2
    }
}

trait Baz: Foo + Bar {}

impl<T> Baz for T
where
    T: Foo,
    T: Bar,
{}

fn baz<T>(x: T) -> T
where
    T: Baz,
{
    Bar::bar(Foo::foo(x))
}

fn main() {
    assert(0.foo().bar() == baz(0));
}
