struct Bar {
    inner: [u8; 3],
}

struct Foo {
    a: Field,
    b: [Field; 3],
    bar: Bar,
}

fn main(mut x: [Foo; 3], y: pub Field, hash_result: pub [u8; 32]) {
    x[y - 1].bar.inner = [106, 107, 10];
    let mut hash_input: [u8; 3] = x[y - 1].bar.inner;
    hash_input[y - 1] = 0;
    let hash: [u8; 32] = std::hash::blake3(hash_input);
    assert(hash == hash_result);
}
