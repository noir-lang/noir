fn main() {
    let x: u32 = 6_u32;
    let y: u32 = 2_u32;
    // Safety: comment added by `nargo expand`
    unsafe {
        assert((x + y) == add(x, y));
        assert((x - y) == sub(x, y));
        assert((x * y) == mul(x, y));
        assert((x / y) == div(x, y));
        assert(check_signed_div(6_i32, 2_i32, 3_i32));
        assert(eq(1_u32, 2_u32) == false);
        assert(eq(1_u32, 1_u32));
        assert(lt(x, y) == false);
        assert(lt(y, x));
        assert((x & y) == and(x, y));
        assert((x | y) == or(x, y));
        assert(check_xor(x, y, 4_u32));
        assert((x >> y) == shr(x, y));
        assert((x << y) == shl(x, y));
    }
}

unconstrained fn add(x: u32, y: u32) -> u32 {
