fn main() {
    let z1: i32 = 0;
    let z2: i32 = 1;
    let cl_outer: fn[(i32, i32)](i32) -> i32 = |x: i32| -> i32 {
        let cl_inner: fn[(i32, i32)](i32) -> i32 = |y: i32| -> i32 { (x + y) + z2 };
        cl_inner(1) + z1
    };
    let result: i32 = cl_outer(1);
    assert(result == 3);
}
