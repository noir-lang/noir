fn main() {
    let z1: Field = 0;
    let z2: Field = 1;
    let cl_outer: fn[(Field, Field)](Field) -> Field = |x: Field| -> Field {
        let cl_inner: fn[(Field, Field)](Field) -> Field = |y: Field| -> Field { (x + y) + z2 };
