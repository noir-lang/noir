fn main() {
    let a: str<5> = "hello";
    let b: [u8; 5] = a.as_bytes();
    assert(b[0_u32] == 104_u8);
    assert(b[1_u32] == 101_u8);
    assert(b[2_u32] == 108_u8);
    assert(b[3_u32] == 108_u8);
    assert(b[4_u32] == 111_u8);
    assert(b.len() == 5_u32);
    let mut c: [u8] = a.as_bytes_vec();
    assert(c[0_u32] == 104_u8);
    assert(c[1_u32] == 101_u8);
    assert(c[2_u32] == 108_u8);
    assert(c[3_u32] == 108_u8);
    assert(c[4_u32] == 111_u8);
    assert(c.len() == 5_u32);
}
