struct Point {
    x: Field,
    y: Field,
}

unconstrained fn sum_vector(vector: [[Point; 2]]) -> Field {
    let mut sum: Field = 0_Field;
    for i in 0_u32..vector.len() {
        for j in 0_u32..vector[i].len() {
            sum = sum + (vector[i][j].x + vector[i][j].y);
        }
    }
    sum
}

fn main() {
    let mut vector: [[Point; 2]] = &[];
    vector =
        vector.push_back([Point { x: 13_Field, y: 14_Field }, Point { x: 20_Field, y: 8_Field }]);
    // Safety: comment added by `nargo expand`
    unsafe {
        let brillig_sum: Field = sum_vector(vector);
        assert(brillig_sum == 55_Field);
    };
    vector =
        vector.push_back([Point { x: 15_Field, y: 5_Field }, Point { x: 12_Field, y: 13_Field }]);
    // Safety: comment added by `nargo expand`
    unsafe {
        let brillig_sum: Field = sum_vector(vector);
        assert(brillig_sum == 100_Field);
    }
}
