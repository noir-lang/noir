mod moo {
    pub comptime fn add_fn(f: FunctionDefinition, name: Quoted) -> Quoted {
        assert(f.has_named_attribute("add_fn"));
        quote {
            pub fn $name() {
                
            }
        }
    }

    pub fn bar() {}

    pub fn foo() {}
}

comptime fn add_foo(_: Module) -> Quoted {
    let func: fn(FunctionDefinition, Quoted) -> Quoted = moo::add_fn;
    quote {
        #[$func(quote {
            bar
        })]pub fn foo() {
            
        }
    }
}

fn main() {
    moo::foo();
    moo::bar();
}
