comptime fn my_macro(x: Field, y: Field) -> Quoted {
    quote { $x + $y + a + b }
}

fn main() {
    let a: Field = 100_Field;
    let b: Field = 200_Field;
    let result: Field = { ((1_Field + 2_Field) + a) + b };
    assert(result == (((1_Field + 2_Field) + a) + b));
}
