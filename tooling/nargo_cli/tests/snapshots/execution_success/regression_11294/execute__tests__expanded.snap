impl PrivateKernelCircuitPublicInputsBuilder {
    pub fn finish(self) -> PrivateKernelCircuitPublicInputs {
        PrivateKernelCircuitPublicInputs { end: self.end.finish() }
    }
}

pub struct PrivateAccumulatedDataBuilder {
