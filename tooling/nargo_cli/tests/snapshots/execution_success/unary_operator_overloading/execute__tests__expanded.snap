use std::ops::{Neg, Not};

fn main(x: u32) {
    let wx: Wrapper = Wrapper::new(x as i32);
    let ex: i32 = 3;
    assert(-wx.inner == -ex);
    assert(!wx.inner == !ex);
    let x: i32 = 3;
    assert(-3 == -x);
}

struct Wrapper {
    inner: i32,
}

impl Wrapper {
    fn new(inner: i32) -> Self {
        Self { inner: inner }
    }
}

impl Neg for Wrapper {
    fn neg(self) -> Self {
        Self::new(-self.inner)
    }
}

impl Not for Wrapper {
    fn not(self) -> Self {
        Self::new(!self.inner)
    }
}
