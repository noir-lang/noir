    assert(((two << 3_u64) / 8_u64) == two);
    assert((three >> 1_u64) == 1_u64);
    assert((x << 1_u64) == 128_u64);
    assert((x >> 2_u64) == 16_u64);
    regression_2250();
    assert((x << 63_u64) == 0_u64);
