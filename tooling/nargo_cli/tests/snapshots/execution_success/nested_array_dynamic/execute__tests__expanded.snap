    assert(x[y].bar.inner == [109_Field, 110_Field, 111_Field]);
    if y != 2_u32 {
        x[y].a = 50_Field;
    } else {
        x[y].a = 100_Field;
    };
    assert(x[3_u32].a == 50_Field);
    if y == 2_u32 {
        {
            let i_4286: u32 = y - 1_u32;
            x[i_4286].b = [50_Field, 51_Field, 52_Field];
        }
    } else {
        {
            let i_4287: u32 = y - 1_u32;
            x[i_4287].b = [100_Field, 101_Field, 102_Field];
        }
