fn main(input: [u32; 4]) {
    let mut vector1: [u32] = [1_u32, 2_u32, 3_u32, 4_u32].as_vector();
    if vector1[0_u32] == 3_u32 {
        vector1[1_u32] = 4_u32;
    };
    if vector1[1_u32] == 5_u32 {
        vector1[3_u32] = 6_u32;
    };
    for i in 0_u32..4_u32 {
        assert(vector1[i] == input[i]);
    }
}
