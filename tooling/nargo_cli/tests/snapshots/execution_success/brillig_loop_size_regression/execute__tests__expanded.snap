        EnumEmulation { a: Option::some(1_Field), b: Option::none(), c: Option::none() };
    for _ in 0_u32..1_u32 {
        assert(emulated_enum.a.unwrap() == 1_Field);
    }
    emulated_enum.a = Option::some(2_Field);
    emulated_enum.a.unwrap()
