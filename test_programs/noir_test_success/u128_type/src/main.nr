#[test]
fn test_u128_sub() {
    let a: u128 = 671967750576550571863734675757137222;
    let b = 671967750576550571863734675757137221;
    let c = a - 1;
    assert(c == b);
}

#[test]
fn test_u128_add() {
    let a: u128 = 671967750576550571863734675757137222;
    let b: u128 = 671967750576550571863734675757137221;
    let c: u128 = b + 1;
    assert(c == a);
}

#[test]
fn test_u128_mul() {
    let a: u128 = 671967750576550571863734675757137222;
    let b: u128 = 335983875288275285931867337878568611;
    let c: u128 = b * 2;
    assert(c == a);
}

#[test]
fn test_u128_div() {
    let a: u128 = 671967750576550571863734675757137222;
    let b: u128 = 335983875288275285931867337878568611;
    let c = a / 2;
    assert(c == b);
}

#[test]
fn test_u128_not() {
    let a: u128 = 1;
    let b = !a;
    assert_eq(b, 340282366920938463463374607431768211454);

    let a: u128 = 340282366920938463463374607431768211454;
    let b = !a;
    assert_eq(b, 1);
}

#[test]
fn test_max_u128() {
    let a: u128 = 340282366920938463463374607431768211455;
    let c = 170141183460469231731687303715884105727;
    assert_eq(a / 2, c);
}
