{
  "compilerOptions": {
    "moduleResolution": "node",
    "outDir": "lib",
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "resolveJsonModule": true,
  }
}