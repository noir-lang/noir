    "@web/test-runner-playwright": "^0.11.1",
    "adm-zip": "^0.5.16",
    "assert": "^2.1.0",
    "browserify-fs": "^1.0.0",
    "chai": "^4.5.0",
    "copy-webpack-plugin": "^13.0.1",
    "eslint": "^9.39.2",
    "eslint-plugin-prettier": "^5.5.4",
    "html-webpack-plugin": "^5.6.5",
    "memfs": "^4.17.2",
    "mocha": "^11.5.0",
    "mocha-each": "^2.0.1",
    "path-browserify": "^1.0.1",
    "prettier": "3.7.4",
    "process": "^0.11.10",
    "readable-stream": "^4.4.2",
    "sinon": "^21.0.0",
    "ts-loader": "^9.5.2",
