    "copy-webpack-plugin": "^11.0.0",
    "html-webpack-plugin": "^5.5.4",
    "memfs": "^4.6.0",
    "mocha": "^10.2.0",
    "mocha-each": "^2.0.1",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "readable-stream": "^4.4.2",
    "sinon": "^17.0.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2",
